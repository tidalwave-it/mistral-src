<project name="FaxDemo" default="default" basedir=".">
    <description>Builds, tests, and runs the project Mistral - Examples - FaxDemo.</description>
    <import file="nbproject/build-impl.xml"/>

    <import file="nbproject/profiler-build-impl.xml"/>
    <property file="keystore.properties" />

    <target name="sign" depends="jar">
        <signjar destDir="dist" alias="FaxManager" keystore="${keystore}" preservelastmodified="true" storepass="${password}">
          <path>
            <fileset dir="dist" includes="**/*.jar" />
          </path>
        </signjar>
    </target>
</project>
