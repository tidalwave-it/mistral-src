<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
    <servlet>
        <servlet-name>GetFaxServlet</servlet-name>
        <servlet-class>it.tidalwave.mistral.faxserver.GetFaxServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PutFaxServlet</servlet-name>
        <servlet-class>it.tidalwave.mistral.faxserver.PutFaxServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StampServlet</servlet-name>
        <servlet-class>it.tidalwave.mistral.faxserver.StampServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Pack200Servlet</servlet-name>
        <servlet-class>it.tidalwave.mistral.faxserver.Pack200Servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetFaxServlet</servlet-name>
        <url-pattern>/GetFax</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PutFaxServlet</servlet-name>
        <url-pattern>/PutFax</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StampServlet</servlet-name>
        <url-pattern>/Stamp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Pack200Servlet</servlet-name>
        <url-pattern>/lib/*</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
	<welcome-file>
            index.jsp
        </welcome-file>
    </welcome-file-list>
</web-app>
